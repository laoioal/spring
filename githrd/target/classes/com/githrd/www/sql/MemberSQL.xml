<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
			"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="mSQL">
	<select id="login" resultType="int" parameterType="mVO">
		SELECT
			COUNT(*)
		FROM
			member
		WHERE
			isshow = 'Y'
			AND id = #{id}
			AND pw = #{pw}
	</select>
</mapper>